{
    "version": 1,
    "builds": [
        {
            "src": "Dockerfile",
            "use": "@vercel/docker",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.)",
            "dest": "http://localhost:$PORT"
    }
    ]
}